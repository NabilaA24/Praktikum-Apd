    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="HargaBeras, HargaDiskonBerasMawar, HargaDiskonBerasSania, HargaDiskonBerasMaknyus, DiskonBerasMawar, DiskonBerasSania, DiskonBerasMaknyus" type="Integer" array="False" size=""/>
            <declare name="PersentaseDiskonMawar, PersentaseDiskonSania, PersentaseDiskonMaknyus" type="Real" array="False" size=""/>
            <declare name="nama, NIM" type="String" array="False" size=""/>
