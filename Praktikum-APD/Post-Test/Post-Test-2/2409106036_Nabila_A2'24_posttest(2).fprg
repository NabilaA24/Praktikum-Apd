            <output expression="&quot;Jika dia membeli beras Sania ia harus membayar &quot; &amp; HargaDiskonBerasSania &amp; &quot; Setelah mendapat diskon 14%.&quot;" newline="True"/>
            <output expression="&quot;Jika dia membeli beras Maknyus ia harus membayar &quot; &amp; HargaDiskonBerasMaknyus &amp; &quot; Setelah mendapat diskon 11%.&quot;" newline="True"/>
