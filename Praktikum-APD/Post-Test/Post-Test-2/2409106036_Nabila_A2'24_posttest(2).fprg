            <output expression="&quot;masukkan NIM : &quot;" newline="True"/>
            <input variable="NIM"/>
            <output expression="&quot;masukkan harga beras :&quot;" newline="True"/>
            <input variable="HargaBeras"/>
            <assign variable="PersentaseDiskonMawar" expression="0.11"/>
            <assign variable="PersentaseDiskonSania" expression="0.14"/>
            <assign variable="PersentaseDiskonMaknyus" expression="0.17"/>
            <assign variable="DiskonBerasMawar" expression="HargaBeras*PersentaseDiskonMawar"/>
            <assign variable="DiskonBerasSania" expression="HargaBeras*PersentaseDiskonSania"/>
            <assign variable="DiskonBerasMaknyus" expression="HargaBeras*PersentaseDiskonMaknyus"/>
